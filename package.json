{
  "name": "@citolab/qti-components",
  "repository": "https://github.com/Citolab/qti-components",
  "description": "QTI webcomponents",
  "author": "Patrick de Klein",
  "contributors": [
    "Sander Looise",
    "Marcel Hoekstra"
  ],
  "license": "GPLv3",
  "private": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "version": "6.4.5",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./qti-components": {
      "import": "./dist/qti-components/index.js",
      "require": "./dist/qti-components/index.cjs"
    },
    "./qti-transform": {
      "import": "./dist/qti-transform/index.js",
      "require": "./dist/qti-transform/index.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "types": "./dist/qti-components/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "qti-components": [
        "./dist/qti-components/index.d.ts"
      ],
      "qti-transform": [
        "./dist/qti-transform/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "./dist/styles.css",
    "./dist/styles.bundled.css"
  ],
  "scripts": {
    "test:generate-output": "jest --watch --json --outputFile=.jest-test-results.json || true",
    "test:output": "jest --json --outputFile=.jest-test-results.json",
    "test": "jest",
    "dev": "vite",
    "build-app": "tsc && vite build",
    "build-lib": "ts-node --esm --project tsconfig.node.json scripts/build.ts",
    "watch-lib": "nodemon --watch \"src/**\" --ext \"ts,json,css\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node --esm --project tsconfig.node.json scripts/watch.ts\"",
    "storybook": "storybook dev -p 6006",
    "deploy": "firebase deploy --only hosting:qti-components",
    "build-storybook": "storybook build",
    "npm publish": "np",
    "test-storybook": "test-storybook",
    "cem": "cem analyze --watch",
    "lint": "eslint src/lib/**",
    "kill 6006": "sudo lsof -i :6006 | grep LISTEN | awk '{print $2}' | xargs kill",
    "list-outdated-dependencies": "npx npm-check-updates --format repo --peer",
    "update-dependencies": "npx npm-check-updates --peer -u && npm install",
    "arethetypeswrong": "npm run build-lib && attw --pack",
    "----hooks----": "",
    "prepublishOnly": "echo 'calling prepublishOnly as npm lifecycle hook to build-lib!' && npm run build-lib",
    "postpublish": "npm run deploy"
  },
  "dependencies": {
    "@lit-labs/preact-signals": "^1.0.0",
    "@lit/context": "^1.0.0",
    "@lit/react": "^1.0.0",
    "cheerio": "^1.0.0-rc.12",
    "lit": "^3.0.0"
  },
  "peerdependencies": {
    "@lit/context": ">= 1.0",
    "@lit/react": ">= 1.0",
    "lit": ">= 3 < 4",
    "react": "> 18",
    "@lit-labs/preact-signals": "^1.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.12.2",
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@jest/globals": "^29.7.0",
    "@storybook/addon-a11y": "^7.5.1",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-jest": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/web-components": "^7.5.1",
    "@storybook/web-components-vite": "^7.5.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "custom-element-vs-code-integration": "^1.2.1",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-lit-a11y": "^4.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^2.0.4",
    "haunted": "^5.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lit-xml": "^0.6.0",
    "nodemon": "^3.0.1",
    "np": "^8.0.4",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^4.0.0",
    "storybook": "^7.5.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "customElements": "custom-elements.json"
}
