{
  "name": "@citolab/qti-components",
  "repository": "https://github.com/Citolab/qti-components",
  "description": "QTI webcomponents",
  "author": "Patrick de Klein",
  "contributors": [
    "Sander Looise",
    "Marcel Hoekstra"
  ],
  "license": "GPLv3",
  "private": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "version": "6.6.1-24",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./qti-components": "./dist/qti-components/index.js",
    "./qti-transformers": "./dist/qti-transformers/index.js",
    "./item.css": "./dist/item.css",
    "./item.minimal.css": "./dist/item.minimal.css"
  },
  "types": "./dist/qti-components/index.d.ts",
  "typesVersions": {
    "*": {
      "qti-components": [
        "./dist/qti-components/index.d.ts"
      ],
      "qti-transformers": [
        "./dist/qti-transformers/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "./dist/item.css",
    "./dist/item.minimal.css"
  ],
  "scripts": {
    "test": "jest",
    "test:output": "jest --json --outputFile=.jest-test-results.json",
    "test:watch-output": "jest --watch --json --outputFile=.jest-test-results.json || true",
    "dev": "vite",
    "build-app": "tsc && vite build",
    "build-lib": "ts-node --esm --project tsconfig.node.json scripts/build.ts",
    "watch-lib": "nodemon --watch \"src/**\" --ext \"ts,json,css\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node --esm --project tsconfig.node.json scripts/build.ts\" watch",
    "deploy": "firebase deploy --only hosting:qti-components",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "npm publish": "np",
    "cem": "cem analyze --watch",
    "lint": "eslint src/lib/**",
    "kill 6006": "sudo lsof -i :6006 | grep LISTEN | awk '{print $2}' | xargs kill",
    "list-outdated-dependencies": "npx npm-check-updates --format repo --peer",
    "update-dependencies": "npx npm-check-updates --peer -u && npm install",
    "arethetypeswrong": "npm run build-lib && attw --pack",
    "----hooks----": "",
    "prepublishOnly": "echo 'calling prepublishOnly as npm lifecycle hook to build-lib!' && npm run build-lib",
    "postpublish": "echo 'posthook for prosperity, does nothing for now!'"
  },
  "dependencies": {},
  "peerdependencies": {
    "@lit/context": "^1.1.0",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@lit/context": "^1.1.0",
    "lit": "^3.1.0",
    "@arethetypeswrong/cli": "^0.13.2",
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@jest/globals": "^29.7.0",
    "@storybook/addon-a11y": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-jest": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/preview-api": "^7.6.3",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/web-components": "^7.6.3",
    "@storybook/web-components-vite": "^7.6.3",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.9.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "autoprefixer": "^10.4.16",
    "custom-element-vs-code-integration": "^1.2.1",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-lit-a11y": "^4.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^2.0.4",
    "jest-environment-jsdom": "^29.7.0",
    "lit-xml": "^0.6.0",
    "nodemon": "^3.0.1",
    "np": "^8.0.4",
    "path-browserify": "^1.0.1",
    "playwright": "^1.40.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^4.0.0",
    "storybook": "^7.6.3",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wc-storybook-helpers": "^1.1.1"
  },
  "customElements": "custom-elements.json"
}
