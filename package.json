{
  "name": "@citolab/qti-components",
  "repository": "https://github.com/Citolab/qti-components",
  "description": "QTI webcomponents",
  "author": "Patrick de Klein",
  "contributors": [
    "Sander Looise",
    "Marcel Hoekstra"
  ],
  "license": "GPLv3",
  "private": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "version": "6.7.1-51",
  "type": "module",
  "exports": {
    "./qti-components": "./dist/qti-components/index.js",
    "./qti-transformers": "./dist/qti-transformers/index.js",
    "./item.css": "./dist/item.css"
  },
  "types": "./dist/qti-components/index.d.ts",
  "typesVersions": {
    "*": {
      "qti-components": [
        "./dist/qti-components/index.d.ts"
      ],
      "qti-transformers": [
        "./dist/qti-transformers/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "./dist/item.css"
  ],
  "scripts": {
    "test": "vitest",
    "test-browser": "vitest --browser.name=chrome",
    "dev": "vite",
    "build": "wireit",
    "build-lib": "tsx scripts/build.ts",
    "watch-lib": "nodemon --watch \"src/**\" --ext \"ts,json,css\" --ignore \"src/**/*.spec.ts\" --exec \"tsx scripts/build.ts\" watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "cem": "cem analyze --watch",
    "lint": "eslint src/lib/**",
    "arethetypeswrong": "wireit",
    "----hooks----": "-",
    "prepublishOnly": "echo 'calling prepublishOnly as npm lifecycle hook to build-lib!' && npm run build-lib",
    "postpublish": "echo 'posthook for prosperity, does nothing for now!'"
  },
  "wireit": {
    "build": {
      "command": "tsc"
    },
    "arethetypeswrong": {
      "command": "attw --pack",
      "dependencies": [
        "build-lib"
      ]
    }
  },
  "peerDependencies": {
    "@lit/context": "^1.1.0",
    "lit": "^3.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@chromatic-com/storybook": "^1.3.3",
    "@custom-elements-manifest/analyzer": "^0.9.8",
    "@lit/context": "^1.1.1",
    "@storybook/addon-a11y": "^8.0.10",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/preview-api": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/web-components": "^8.0.10",
    "@storybook/web-components-vite": "^8.0.10",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@unocss/postcss": "^0.59.4",
    "@unocss/preset-typography": "^0.59.4",
    "@unocss/preset-wind": "^0.59.4",
    "@unocss/reset": "^0.59.4",
    "@unocss/transformer-directives": "^0.59.4",
    "@vitest/browser": "^1.6.0",
    "@wdio/cli": "^8.36.1",
    "autoprefixer": "^10.4.19",
    "custom-element-vs-code-integration": "^1.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lit": "^1.11.0",
    "eslint-plugin-lit-a11y": "^4.1.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-wc": "^2.1.0",
    "lit": "^3.1.3",
    "nodemon": "^3.1.0",
    "np": "^10.0.5",
    "path-browserify": "^1.0.1",
    "playwright": "^1.44.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.2",
    "prettier": "3.2.5",
    "remark-gfm": "^4.0.0",
    "storybook": "^8.0.10",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "wireit": "^0.14.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.17.2"
  },
  "customElements": "custom-elements.json"
}
