{
  "hosting": {
    "public": "storybook-static",
    "target": "qti-components",
    "predeploy": ["npm run storybook:build"],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
