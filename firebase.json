{
  "hosting": {
    "public": "storybook-static",
    "target": "qti-components",
    "predeploy": [
      "npm run build-storybook"
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
